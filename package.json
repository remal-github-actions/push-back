{
  "$schema": "https://json.schemastore.org/package",
  "name": "push-back",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.1.0",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint --fix 'src/**/*'",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir src --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "debug": "4.3.4",
    "simple-git": "3.22.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "rimraf": "5.0.5",
    "semver": "^7",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  }
}
